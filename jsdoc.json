{
    "source": {
      "include": ["js/src"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules|docs|js/tests)"
    },
    "sourceType": "module",
    "plugins": ["plugins/markdown"],
    "templates": {
      "referenceTitle": "Transport protocols simulator library",
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "recurse": true,
      "destination": "./docs/",
      "template": "jsdoc-template",
      "readme": "./README.md"
    }
  }